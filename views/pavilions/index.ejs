<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>Pavilions</h1>
    <ul>
      <% pavilions.forEach((pavilion) => { %>
      <a href="/pavilions/<%= pavilion.id %>">
        <li><%= pavilion.name %></li>
      </a>
      <% }) %>
      <br />
      <br />
      <a href="/pavilions/new">Create New Pavilion</a>
    </ul>
    <br />
    <a href="/">Home</a>
  </body>
</html>
