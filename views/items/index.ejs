<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Booth</th>
      <th>Pavilion</th>
      <th>Description</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% items.forEach(function(item) { %>
    <tr>
      <td><%= item.name %></td>
      <td>$<%= item.price %></td>
      <td><%= item.booth %></td>
      <td><%= item.pavilion %></td>
      <td><%= item.description %></td>
      <td><a href="/items/<%= item._id %>">DETAILS</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>
